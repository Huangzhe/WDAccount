<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/button_pre</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColorPrimary">@color/abc_primary_text_material_dark</item>
        <item name="android:colorBackground">@color/abc_background_cache_hint_selector_material_light</item>
    </style>

    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="rl_labelStyle">
        <item name="android:layout_marginLeft">@dimen/d20</item>
        <item name="android:layout_marginRight">@dimen/d20</item>
        <item name="android:layout_marginTop">@dimen/d5</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/d40</item>
        <item name="android:background">@drawable/shape_text_bg</item>
    </style>


    <style name="textLabel">
        <item name="android:layout_margin">0.5dp</item>
        <item name="android:background">@drawable/shape_text_label_bg</item>

        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">5dp</item>
        <item name="android:textSize">@dimen/s14</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="textLabel1" parent="textLabel">
        <item name="android:gravity">center</item>

        <item name="android:layout_width">@dimen/d110</item>


    </style>


    <style name="view_divider_style">
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_width">0.5dp</item>
        <item name="android:layout_height">@dimen/d35</item>
        <item name="android:background">@color/layoutstrokecolor</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="editText_shkt">
        <item name="android:layout_margin">0.5dp</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:padding">@dimen/d5</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/s14</item>
        <item name="android:textColor">#333333</item>
        <item name="android:background">@drawable/shape_text_et_bg</item>
    </style>
</resources>
